		<function>
			<name value="getInventorySlotData" />
			<desc value="Gets the damage value of the item in the specified inventory slot." />
			<syntax value="Player.getInventorySlotData();" />
			<returns value="integer" />
		</function>
		<function>
			<name value="getName" />
			<desc value="Gets the player's in-game name." />
			<syntax value="Player.getName(player);" />
			<returns value="string" />
		</function>
		<function>
			<name value="getSelectedSlotId" />
			<desc value="Gets the selected inventory slot." />
			<syntax value="Player.getSelectedSlotId();" />
