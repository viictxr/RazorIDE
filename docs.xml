		<function>
			<name value="getInventorySlot" />
			<desc value="Gets item in the specified inventory slot." />
			<syntax value="Player.getInventorySlot(slot);" />
			<returns value="integer" />
		</function>
		<function>
			<name value="getInventorySlotCount" />
			<desc value="Gets the number of items in the specified inventory slot." />
			<syntax value="Player.getInventorySlotCount(slot);" />
			<returns value="integer" />
