		</function>
		<function>
			<name value="setInventorySlot" />
			<desc value="Sets item in the specified inventory slot." />
			<syntax value="Player.setInventorySlot(slot, id, count, damage);" />
			<returns value="null" />
		</function>
		<function>
			<name value="getEnchantments" />
			<desc value="Returns an array of the enchantments applied to the specified slot." />
			<syntax value="Player.getEnchantments(slot);" />
			<returns value="EnchantmentArray" />
		</function>
		<function>
			<name value="getCustomName" />
			<desc value="Gets the custom name of an item." />
			<syntax value="Player.getItemCustomName(slot);" />
