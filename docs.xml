		</function>
		<function>
			<name value="getScore" />
			<desc value="No information available." />
			<syntax value="Player.getScore();" />
			<returns value="integer" />
		</function>
			<function>
			<name value="isFlying" />
			<desc value="Checks if the player is flying or not." />
			<syntax value="Player.isFlying();" />
			<returns value="boolean" />
		</function>
		<function>
			<name value="setCanFly" />
			<desc value="Sets if the player can fly or not." />
