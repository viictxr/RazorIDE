		</function>
		<function>
			<name value="setExhaustion" />
			<desc value="Sets players exhaustion level." />
			<syntax value="Player.setExhaustion(exhaustion);" />
			<returns value="null" />
		</function>
		<function>
			<name value="setHunger" />
			<desc value="Sets the player's hunger level." />
			<syntax value="Player.setHunger(hunger);" />
			<returns value="null" />
		</function>
		<function>
			<name value="setSaturation" />
